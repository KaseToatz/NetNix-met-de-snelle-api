        "/user/addprofile": {
            "post": {
                "summary": "Add profile",
                "description": "Add profile to user.",
                "operationId": "AddProfile__post",
                "paramaters": [
                    {
                        "name": "Authorization",
                        "in": "header",
                        "description": "User Authentication",
                        "required": true
                    }
                ],
                "requestBody": {
                    "description": "Data required for creating a new profile.",
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "age": {
                                        "type": "string"
                                    },
                                    "profilePicture": {
                                        "type": "string",
                                        "format": "binary"
                                    },
                                    "language": {
                                        "type": "string"
                                    },
                                    "preferences": {
                                        "type": "object",
                                        "properties": {
                                            "genre": {
                                                "type": "array",
                                                "items": {
                                                    "type": "integer"
                                                }
                                            }
                                        },
                                        "nullable": true
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Succesful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {}
                                }
                            }
                        }
                    }
                }
            }
        },
